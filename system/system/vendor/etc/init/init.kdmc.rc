######################################################################
# Config file for DIAG clients
#
# Syntax:
#   [App full path] param1 param2 param3 ...
#
# Example:
#   vendor/bin/ktestXdiag 1 0 3
######################################################################


on property:kc.diag.status=start && property:kc.diag.fact=kcfactoff
    class_start kc_diag

on property:kc.diag.status=start && property:kc.diag.fact=kcfacton
    class_start kc_fact_diag

######################################################################
# For Diag Common
######################################################################
on property:kc.diag.fact=kcfactoff
    class_start kc_diag_common

on property:kc.diag.fact=kcfacton
    class_start kc_fact_diag_common

on property:kc.diag.status=stop && property:kc.diag.fact=kcfactoff
    class_stop kc_diag 

on property:kc.diag.status=stop && property:kc.diag.fact=kcfacton
    class_stop kc_fact_diag

on property:sys.diagofflineservice=1
    class_start kcd_diag

on property:sys.diagofflineservice=0
    class_stop kcd_diag


